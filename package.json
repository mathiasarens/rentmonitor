{
  "name": "rentmonitor-server",
  "version": "0.6.2",
  "description": "Monitor the payments of your tenants. Did every renter pay on time?",
  "keywords": [
    "rent",
    "check",
    "tenant",
    "payments",
    "fints-support"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "NODE_ENV=test lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "NODE_ENV=test lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t rentmonitor-server-loopback-new .",
    "docker:run": "docker run -p 3000:3000 -d rentmonitor-server-loopback-new",
    "migrate": "node ./dist/migrate",
    "migrate:testdb": "NODE_ENV=test node ./dist/migrate -- --rebuild",
    "prestart": "npm run build",
    "start": "PORT=3001 node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasarens/rentmonitor-server-loopback.git"
  },
  "author": "Mathias Arens",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.3.3",
    "@loopback/authorization": "^0.4.10",
    "@loopback/boot": "^1.7.4",
    "@loopback/context": "^2.1.1",
    "@loopback/core": "^1.12.4",
    "@loopback/openapi-v3": "^1.13.0",
    "@loopback/repository": "^1.19.1",
    "@loopback/rest": "^1.26.1",
    "@loopback/rest-explorer": "^1.4.10",
    "@loopback/service-proxy": "^1.3.17",
    "bcryptjs": "^2.4.3",
    "debug": "^4.1.1",
    "isemail": "^3.2.0",
    "js-yaml": ">=3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "loopback-connector-postgresql": "^3.8.3",
    "node-fints": "^0.5.1"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.1",
    "@loopback/eslint-config": "^5.0.3",
    "@loopback/testlab": "^1.10.3",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.5"
  }
}
