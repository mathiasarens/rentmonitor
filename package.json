{
  "name": "rentmonitor-server",
  "version": "1.0.4",
  "description": "Monitor the payments of your tenants. Did every renter pay on time?",
  "keywords": [
    "rent",
    "check",
    "tenant",
    "payments",
    "fints-support"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t rentmonitor-server-loopback .",
    "docker:run": "docker run -p 3000:3000 -d rentmonitor-server-loopback",
    "migrate": "node ./dist/migrate",
    "migrate:testdb": "NODE_ENV=test node ./dist/migrate -- --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "PORT=3001 node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasarens/rentmonitor-server-loopback.git"
  },
  "author": "Mathias Arens",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.1.0",
    "@loopback/authorization": "^0.8.0",
    "@loopback/boot": "^3.3.0",
    "@loopback/cli": "^2.20.0",
    "@loopback/context": "^3.15.0",
    "@loopback/core": "^2.15.0",
    "@loopback/openapi-v3": "^5.2.0",
    "@loopback/repository": "^3.5.0",
    "@loopback/rest": "^9.2.0",
    "@loopback/rest-explorer": "^3.2.0",
    "@loopback/service-proxy": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.1",
    "isemail": "^3.2.0",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-postgresql": "^5.3.0",
    "node-fints": "^0.5.1",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/repository-json-schema": "^3.3.0",
    "@loopback/testlab": "^3.3.0",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-mocha": "^8.1.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.2.3"
  }
}
