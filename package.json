{
  "name": "rentmonitor-server",
  "version": "0.8.0",
  "description": "Monitor the payments of your tenants. Did every renter pay on time?",
  "keywords": [
    "rent",
    "check",
    "tenant",
    "payments",
    "fints-support"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "NODE_ENV=test lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "NODE_ENV=test lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t rentmonitor-server-loopback-new .",
    "docker:run": "docker run -p 3000:3000 -d rentmonitor-server-loopback-new",
    "migrate": "node ./dist/migrate",
    "migrate:testdb": "NODE_ENV=test node ./dist/migrate -- --rebuild",
    "prestart": "npm run build",
    "start": "PORT=3001 node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasarens/rentmonitor-server-loopback.git"
  },
  "author": "Mathias Arens",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.0.5",
    "@loopback/authorization": "^0.7.5",
    "@loopback/boot": "^3.1.2",
    "@loopback/context": "^3.13.2",
    "@loopback/core": "^2.13.1",
    "@loopback/openapi-v3": "^5.1.2",
    "@loopback/repository": "^3.3.0",
    "@loopback/rest": "^9.1.1",
    "@loopback/rest-explorer": "^3.0.5",
    "@loopback/service-proxy": "^3.0.5",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.1",
    "isemail": "^3.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "loopback-connector-postgresql": "^5.2.0",
    "node-fints": "^0.5.1",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.8",
    "@loopback/eslint-config": "^10.0.4",
    "@loopback/repository-json-schema": "^3.2.0",
    "@loopback/testlab": "^3.2.10",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@types/node": "^10.17.48",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-mocha": "^8.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.1.2"
  }
}
